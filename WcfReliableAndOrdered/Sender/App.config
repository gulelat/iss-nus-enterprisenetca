<?xml version="1.0" encoding="utf-8" ?>
<configuration>
 <system.serviceModel>
		<client>
   <endpoint address="net.tcp://localhost:8080/service/imageSender"
    binding="customBinding" bindingConfiguration="ClientBinding"
    contract="ImageSenderContract.IImageSender" name="Destination" />
  </client>
		<bindings>
			<customBinding>
				<binding name="ClientBinding">
				  <!--<reliableSession ordered="true" />-->
					<MyCustomBindingElement />
					<tcpTransport/>
				</binding>
			</customBinding>
		</bindings>
		<extensions>
			<bindingElementExtensions>
				<add name="MyCustomBindingElement" type="Extensibility.MyCustomBindingElementExtensionElement, Sender"/>
			</bindingElementExtensions>
		</extensions>

	</system.serviceModel>
</configuration>